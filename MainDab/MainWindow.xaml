            <ColorAnimationUsingKeyFrames Storyboard.TargetProperty="(Shape.Fill).(SolidColorBrush.Color)" Storyboard.TargetName="GameHubPath1">
                <EasingColorKeyFrame KeyTime="0:0:0.2" Value="#FFABABAB" />
            </ColorAnimationUsingKeyFrames>
