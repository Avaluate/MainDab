        <Storyboard x:Key="ToolsButtonSelected">
            <DoubleAnimation Storyboard.TargetName="ToolsGrid" Storyboard.TargetProperty="(Window.Opacity)" From="0.0" To="1.0" Duration="0:0:0.4" AutoReverse="False" RepeatBehavior="1x" />
            <ColorAnimationUsingKeyFrames Storyboard.TargetProperty="(Shape.Fill).(SolidColorBrush.Color)" Storyboard.TargetName="ToolsPath1">
                <EasingColorKeyFrame KeyTime="0:0:0.2" Value="#FFABABAB" />
            </ColorAnimationUsingKeyFrames>
            <ColorAnimationUsingKeyFrames Storyboard.TargetProperty="(Shape.Fill).(SolidColorBrush.Color)" Storyboard.TargetName="ToolsPath2">
