<Window
        
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:gif="http://wpfanimatedgif.codeplex.com"
        xmlns:local="clr-namespace:MainDabRedo"
        xmlns:avalonedit="http://icsharpcode.net/sharpdevelop/avalonedit" x:Name="MainWin" x:Class="MainDabRedo.MainWindow"
    
        mc:Ignorable="d"
        Title="MainDab" ResizeMode="NoResize" Height="416.65" Width="699.896" Background="{x:Null}" Foreground="#FF939292" Closing="MainWin_Closing" AllowsTransparency="True" WindowStyle="None" Icon="Untitled drawing (40).png" KeyDown="MainWin_KeyDown">

    <Window.Triggers>
        <EventTrigger RoutedEvent="Window.Unloaded">
            <BeginStoryboard>
                <Storyboard Name="FormFadeOut"
                        Completed="CloseWindow">
                    <DoubleAnimation  Name="FormFadeOutAnimation"
                                  Storyboard.TargetName="MainWin"
                                  Storyboard.TargetProperty="(Window.Opacity)"
                                  From="1.0"
                                  To="0.0"
                                  Duration="0:0:0.5"
                                  AutoReverse="False"
                                  RepeatBehavior="1x" />
                </Storyboard>
            </BeginStoryboard>
        </EventTrigger>
    </Window.Triggers>


    <Window.Resources>

        <SolidColorBrush x:Key="StandardBorderBrush" Color="#888" />
        <SolidColorBrush x:Key="StandardBackgroundBrush" Color="Black" />
        <SolidColorBrush x:Key="HoverBorderBrush" Color="#DDD" />
        <SolidColorBrush x:Key="SelectedBackgroundBrush" Color="Gray" />
        <SolidColorBrush x:Key="SelectedForegroundBrush" Color="White" />
        <SolidColorBrush x:Key="DisabledForegroundBrush" Color="#888" />
        <SolidColorBrush x:Key="GlyphBrush" Color="#444" />
        <SolidColorBrush x:Key="NormalBrush" Color="#888" />
        <SolidColorBrush x:Key="NormalBorderBrush" Color="#888" />
        <SolidColorBrush x:Key="HorizontalNormalBrush" Color="#FF686868" />
        <SolidColorBrush x:Key="HorizontalNormalBorderBrush" Color="#888" />
        <Style x:Key="{x:Type ToolTip}"
       TargetType="{x:Type ToolTip}">
            <Setter Property="Background"
            Value="#FF111315" />
            <Setter Property="Foreground"
            Value="#FFD7D7D7" />
        </Style>
        <Storyboard x:Key="APISelection1">
            <DoubleAnimation  Name="APISelection1"
                              Storyboard.TargetName="APISelection"
                              Storyboard.TargetProperty="(Window.Opacity)"
                              From="0.0"
                              To="1.0"
                              Duration="0:0:0.5"
                              AutoReverse="False"
                              RepeatBehavior="1x" />
        </Storyboard>
        <Storyboard x:Key="GeneralOptions1">
            <DoubleAnimation  Name="GeneralOptions1"
                              Storyboard.TargetName="GeneralOptions"
                              Storyboard.TargetProperty="(Window.Opacity)"
                              From="0.0"
                              To="1.0"
                              Duration="0:0:0.5"
                              AutoReverse="False"
                              RepeatBehavior="1x" />
        </Storyboard>
        <Storyboard x:Key="ThemeSelection1">
            <DoubleAnimation  Name="ThemeSelection1"
                              Storyboard.TargetName="ThemeSelection"
                              Storyboard.TargetProperty="(Window.Opacity)"
                              From="0.0"
                              To="1.0"
                              Duration="0:0:0.5"
                              AutoReverse="False"
                              RepeatBehavior="1x" />
        </Storyboard>
        <Storyboard x:Key="HomeButtonSelected">
            <DoubleAnimation Storyboard.TargetName="HomeGrid" Storyboard.TargetProperty="(Window.Opacity)" From="0.0" To="1.0" Duration="0:0:0.4" AutoReverse="False" RepeatBehavior="1x" />
            <ColorAnimationUsingKeyFrames Storyboard.TargetProperty="(Shape.Fill).(SolidColorBrush.Color)" Storyboard.TargetName="Path1">
                <EasingColorKeyFrame KeyTime="0:0:0.2" Value="#FFABABAB" />
            </ColorAnimationUsingKeyFrames>
            <ColorAnimationUsingKeyFrames Storyboard.TargetProperty="(Shape.Fill).(SolidColorBrush.Color)" Storyboard.TargetName="Path2">
                <EasingColorKeyFrame KeyTime="0:0:0.2" Value="#FFABABAB" />
            </ColorAnimationUsingKeyFrames>
            <ColorAnimationUsingKeyFrames Storyboard.TargetProperty="(Shape.Fill).(SolidColorBrush.Color)" Storyboard.TargetName="Path3">
                <EasingColorKeyFrame KeyTime="0:0:0.2" Value="#FFABABAB" />
            </ColorAnimationUsingKeyFrames>
        </Storyboard>

        <Storyboard x:Key="HomeButtonDeselected">
            <ColorAnimationUsingKeyFrames Storyboard.TargetProperty="(Shape.Fill).(SolidColorBrush.Color)" Storyboard.TargetName="Path1">
                <EasingColorKeyFrame KeyTime="0:0:0.2" Value="#FF5C5B5B" />
            </ColorAnimationUsingKeyFrames>
            <ColorAnimationUsingKeyFrames Storyboard.TargetProperty="(Shape.Fill).(SolidColorBrush.Color)" Storyboard.TargetName="Path2">
                <EasingColorKeyFrame KeyTime="0:0:0.2" Value="#FF5C5B5B" />
            </ColorAnimationUsingKeyFrames>
            <ColorAnimationUsingKeyFrames Storyboard.TargetProperty="(Shape.Fill).(SolidColorBrush.Color)" Storyboard.TargetName="Path3">
                <EasingColorKeyFrame KeyTime="0:0:0.2" Value="#FF5C5B5B" />
            </ColorAnimationUsingKeyFrames>
        </Storyboard>

        <Storyboard x:Key="ExecutorButtonSelected">
            <DoubleAnimation Storyboard.TargetName="ExecutorGrid" Storyboard.TargetProperty="(Window.Opacity)" From="0.0" To="1.0" Duration="0:0:0.4" AutoReverse="False" RepeatBehavior="1x" />
            <ColorAnimationUsingKeyFrames Storyboard.TargetProperty="(Shape.Fill).(SolidColorBrush.Color)" Storyboard.TargetName="ExecutorPath1">
                <EasingColorKeyFrame KeyTime="0:0:0.2" Value="#FFABABAB" />
            </ColorAnimationUsingKeyFrames>
        </Storyboard>

        <Storyboard x:Key="ExecutorButtonDeselected">
            <ColorAnimationUsingKeyFrames Storyboard.TargetProperty="(Shape.Fill).(SolidColorBrush.Color)" Storyboard.TargetName="ExecutorPath1">
