        </Storyboard>

        <Storyboard x:Key="ScriptHubButtonDeselected">
            <ColorAnimationUsingKeyFrames Storyboard.TargetProperty="(Shape.Fill).(SolidColorBrush.Color)" Storyboard.TargetName="ScriptHubPath1">
                <EasingColorKeyFrame KeyTime="0:0:0.2" Value="#FF5C5B5B" />
            </ColorAnimationUsingKeyFrames>
            <ColorAnimationUsingKeyFrames Storyboard.TargetProperty="(Shape.Fill).(SolidColorBrush.Color)" Storyboard.TargetName="ScriptHubPath2">
